{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "pictures",
  "apis": [
    {
      "path": "/pictures/create.json",
      "operations": [
        {
          "summary": "Upload picture on the server",
          "notes": "You need to chose between assoc_id, shelter_id & event_id",
          "parameters": [
            {
              "paramType": "query",
              "name": "token",
              "type": "string",
              "description": "Your token",
              "required": true
            },
            {
              "paramType": "query",
              "name": "file",
              "type": "string",
              "description": "File content (base64)",
              "required": true
            },
            {
              "paramType": "query",
              "name": "filename",
              "type": "string",
              "description": "Name to give to the file",
              "required": true
            },
            {
              "paramType": "query",
              "name": "original_filename",
              "type": "string",
              "description": "Original name of the file",
              "required": true
            },
            {
              "paramType": "query",
              "name": "assoc_id",
              "type": "integer",
              "description": "Assoc's id",
              "required": false
            },
            {
              "paramType": "query",
              "name": "shelter_id",
              "type": "integer",
              "description": "Shelter's id",
              "required": false
            },
            {
              "paramType": "query",
              "name": "event_id",
              "type": "integer",
              "description": "Event's id",
              "required": false
            },
            {
              "paramType": "query",
              "name": "is_main",
              "type": "boolean",
              "description": "true to make it the main picture",
              "required": false
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            }
          ],
          "nickname": "Pictures#create",
          "method": "get"
        }
      ]
    },
    {
      "path": "/pictures/delete.json",
      "operations": [
        {
          "summary": "Delete picture",
          "notes": "Can't delete main picture",
          "parameters": [
            {
              "paramType": "query",
              "name": "token",
              "type": "string",
              "description": "Your token",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            }
          ],
          "nickname": "Pictures#delete",
          "method": "get"
        }
      ]
    },
    {
      "path": "/pictures/update.json",
      "operations": [
        {
          "summary": "Set picture as main picture",
          "parameters": [
            {
              "paramType": "query",
              "name": "token",
              "type": "string",
              "description": "Your token",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 200,
              "responseModel": null,
              "message": "Ok"
            }
          ],
          "nickname": "Pictures#update",
          "method": "get"
        }
      ]
    }
  ],
  "authorizations": null
}